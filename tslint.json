{
	"rules": {
		"arrow-parens": [
			true,
			"ban-single-arg-parens"
		],
		"curly": [
			true,
			"ignore-same-line"
		],
		"eofline": false,
		"indent": [
			true,
			"tabs",
			4
		],
		"max-line-length": false,
		"member-access": [
			true,
			"check-accessor",
			"check-constructor",
			"check-parameter-property"
		],
		"member-ordering": [
			true,
			{
				"order": [
					"private-instance-field",
					"public-instance-field",
					"private-static-field",
					"public-static-field",
					"private-constructor",
					"public-constructor",
					"private-instance-method",
					"public-instance-method",
					"private-static-method",
					"public-static-method"
				]
			}
		],
		"no-console": false,
		"no-string-literal": false,
		"no-trailing-whitespace": [
			true,
			"ignore-blank-lines",
			"ignore-comments",
			"ignore-jsdoc"
		],
		"no-var-keyword": true,
		"object-literal-sort-keys": [
			true,
			"match-declaration-order"
		],
		"prefer-const": true,
		"quotemark": [
			true,
			"double"
		],
		"semicolon": [
			true,
			"always"
		],
		"space-within-parens": [
			false
		],
		"trailing-comma": false,
		"triple-equals": [
			true,
			"allow-null-check",
			"allow-undefined-check"
		],
		"typedef": [
			true,
			"call-signature",
			"arrow-call-signature",
			"parameter",
			"arrow-parameter",
			"property-declaration",
			"variable-declaration",
			"member-variable-declaration",
			"object-destructuring",
			"array-destructuring"
		],
		"typedef-whitespace": [
			true,
			{
				"call-signature": "nospace",
				"index-signature": "nospace",
				"parameter": "nospace",
				"property-declaration": "nospace",
				"variable-declaration": "nospace"
			},
			{
				"call-signature": "onespace",
				"index-signature": "onespace",
				"parameter": "onespace",
				"property-declaration": "onespace",
				"variable-declaration": "onespace"
			}
		],
		"typeof-compare": true,
		"use-isnan": true,
		"whitespace": [
			true,
			"check-branch",
			"check-decl",
			"check-operator",
			"check-operator",
			"check-separator",
			"check-rest-spread",
			"check-type",
			"check-typecast",
			"check-type-operator",
			"check-preblock"
		]
	}
}